{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "single_node_template_x86_1",
      "debugType": "x86simulator",
      "type": "gdb",
      "x86SimOptions": "${workspaceFolder}/single_node_template/build/x86sim/Work/options/x86sim.options",
      "request": "launch",
      "target": "${workspaceFolder}/single_node_template/build/x86sim/Work/pthread/sim.out",
      "cwd": "${workspaceFolder}/single_node_template/build/x86sim",
      "gdbpath": "${env:XILINX_VITIS}/bin/xgdb",
      "valuesFormatting": "prettyPrinters"
    },
    {
      "type": "tcf-debug",
      "request": "launch",
      "name": "single_node_template_aiesim_1",
      "debugType": "aiesimulator",
      "autoAttachProcessChildren": false,
      "target": {
        "targetConnectionId": "XRTServer",
        "peersIniPath": "../../../.peers.ini",
        "context": "MemSock"
      },
      "aieSimulatorOptions": {
        "enablePipelineView": false,
        "packageDirectory": "./Work",
        "inputDirectory": "../..",
        "additionalArgs": "",
        "trace": {
          "enableTrace": false,
          "traceType": "onlineWdb",
          "vcdFileName": "foo",
          "generateXpe": false,
          "traceModules": {
            "io": true,
            "dma": true,
            "memory": true,
            "core": true,
            "shim": true,
            "streamSwitch": true
          },
          "columnSelectionForTrace": [],
          "rowSelectionForTrace": [],
          "timeWindowForTrace": {
            "start": "",
            "end": ""
          }
        },
        "profile": {
          "enableProfiling": false,
          "profileAllCores": true,
          "cores": ""
        }
      },
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}